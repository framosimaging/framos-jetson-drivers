/dts-v1/;
/plugin/;

#include <dt-bindings/clock/tegra234-clock.h>
#include <dt-bindings/gpio/tegra234-gpio.h>
#include <dt-bindings/tegra234-p3767-0000-common.h>


/ {
	overlay-name = "Framos IMX676-CAM0-2Lane";
	jetson-header-name = "Jetson 24pin CSI Connector";
	compatible = JETSON_COMPATIBLE_P3768;

	fragment@0 {
		target = <&sensor_module0>;
		__overlay__ {
			status = "okay";
			badge = "imx676_front_framos";
			drivernode0 {
				status = "okay";
				/* Declare PCL support driver (classically known as guid) */
				devname = "imx676 10-001a";
				/* Declare the device-tree hierarchy to driver instance */
				sysfs-device-tree = "/sys/firmware/devicetree/base/bus@0/cam_i2cmux/i2c@0/imx676_b@1a";
			};
		};
	};

	fragment@1 {
		target = <&sensor_vi_port1>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@2 {
		target = <&sensor_vi_in1>;
		__overlay__ {
			status = "okay";
			bus-width = <2>;
		};
	};

	fragment@3 {
		target = <&sensor_csi_ch1>;
		__overlay__ {
			status = "okay";
			ports {
				status = "okay";
			};
		};
	};

	fragment@4 {
		target = <&sensor_csi_ch1_port0>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@5 {
		target = <&sensor_csi_ch1_in>;
		__overlay__ {
			status = "okay";
			bus-width = <2>;
			remote-endpoint = <&sensor_out_b>;
		};
	};

	fragment@6 {
		target = <&sensor_csi_ch1_port1>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@7 {
		target = <&sensor_csi_ch1_out>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@8 {
		target = <&cam_i2cmux_i2c0>;
		__overlay__ {
			sensor_b: imx676_b@1a {
				compatible = "framos,imx676";
				reg = <0x1a>;

				/* Define any required hw resources needed by driver */
				/* ie. clocks, io pins, power sources */
				clocks = <&bpmp TEGRA234_CLK_EXTPERIPH1>, <&bpmp TEGRA234_CLK_EXTPERIPH1>;
				clock-names = "extperiph1", "pllp_grtba";
				mclk = "extperiph1";
				clock-frequency = <37125000>;

				devnode = "video0";

				physical_w = "15.00";
				physical_h = "12.50";

				sensor_model ="imx676";

				/* Defines number of frames to be dropped by driver internally after applying */
				/* sensor crop settings. Some sensors send corrupt frames after applying */
				/* crop co-ordinates */
				post_crop_frame_drop = "0";

				/* Convert Gain to unit of dB (decibel) befor passing to kernel driver */
				use_decibel_gain = "true";

				/* if true, delay gain setting by one frame to be in sync with exposure */
				delayed_gain = "false";

				/* enable CID_SENSOR_MODE_ID for sensor modes selection */
				use_sensor_mode_id = "false";

				mode0 {
					mclk_khz = "37125";
					num_lanes = "2";
					tegra_sinterface = "serial_b";
					vc_id = "0";
					phy_mode = "DPHY";
					discontinuous_clk = "no";
					dpcm_enable = "false";
					cil_settletime = "0";
					lane_polarity = "6";
					active_w = "3552";
					active_h = "3556";

					dynamic_pixel_bit_depth = "10";
					csi_pixel_bit_depth = "10";
					mode_type = "bayer";
					pixel_phase = "rggb";

					readout_orientation = "0";
					line_length = "4019";
					inherent_gain = "1";
					pix_clk_hz = "475200000";
					serdes_pix_clk_hz = "12000000000"; /* GMSL predefine */

					gain_factor = "10";
					min_gain_val = "0"; /* 0dB */
					max_gain_val = "720"; /* 72dB */
					step_gain_val = "3"; /* 0.3 */
					default_gain = "0";
					min_hdr_ratio = "1";
					max_hdr_ratio = "64";
					framerate_factor = "1000000";
					min_framerate = "1500000"; /* 1.5 */
					max_framerate = "30000000"; /* 30 */
					step_framerate = "1";
					default_framerate= "30000000";
					exposure_factor = "1000000";
					min_exp_time = "27"; /* us */
					max_exp_time = "660000"; /* us */
					step_exp_time = "1";
					default_exp_time = "10000";/* us */
					embedded_metadata_height = "1";
				};
				mode1 {
					mclk_khz = "37125";
					num_lanes = "2";
					tegra_sinterface = "serial_b";
