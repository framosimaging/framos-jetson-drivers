#include <dt-bindings/clock/tegra234-clock.h>
#include <dt-bindings/gpio/tegra234-gpio.h>
#include <dt-bindings/tegra234-p3737-0000+p3701-0000.h>

/* TCA6408 Pxx bus-port define*/
#define CRESETB	 1

/ {
	overlay-name = "Framos IMX530-J5-4Lane";
	jetson-header-name = "Jetson AGX CSI Connector";
	compatible = JETSON_COMPATIBLE;

	fragment@0 {
		target = <&sensor_module0>;
		__overlay__ {
			status = "okay";
			badge = "imx530_bottomleft_framos";
			drivernode0 {
				status = "okay";
				/* Declare PCL support driver (classically known as guid) */
				devname = "imx530 9-001a";
				/* Declare the device-tree hierarchy to driver instance */
				sysfs-device-tree = "/sys/firmware/devicetree/base/bus@0/i2c@3180000/tca9548@70/i2c@0/imx530_a@1a";
			};
		};
	};

	fragment@1 {
		target = <&sensor_vi_port0>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@2 {
		target = <&sensor_vi_in0>;
		__overlay__ {
			status = "okay";
			bus-width = <4>;
		};
	};

	fragment@3 {
		target = <&sensor_csi_ch0>;
		__overlay__ {
			status = "okay";
			ports {
				status = "okay";
			};
		};
	};

	fragment@4 {
		target = <&sensor_csi_ch0_port0>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@5 {
		target = <&sensor_csi_ch0_in>;
		__overlay__ {
			status = "okay";
			bus-width = <4>;
			remote-endpoint = <&sensor_out_a>;
		};
	};

	fragment@6 {
		target = <&sensor_csi_ch0_port1>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@7 {
		target = <&sensor_csi_ch0_out>;
		__overlay__ {
			status = "okay";
		};
	};

