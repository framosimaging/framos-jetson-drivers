/ {
	overlay-name = "Framos IMX577-CAM1-2Lane";
	jetson-header-name = "Jetson 24pin CSI Connector";
	compatible = JETSON_COMPATIBLE_P3768;

	fragment@0 {
		target = <&sensor_module2>;
		__overlay__ {
			status = "okay";
			badge = "imx577_bottomright_framos";
			drivernode0 {
				status = "okay";
				/* Declare PCL support driver (classically known as guid) */
				devname = "imx577 9-001a";
				/* Declare the device-tree hierarchy to driver instance */
				sysfs-device-tree = "/sys/firmware/devicetree/base/bus@0/cam_i2cmux/i2c@1/imx577_c@1a";
			};
		};
	};

	fragment@1 {
		target = <&sensor_vi_port2>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@2 {
		target = <&sensor_vi_in2>;
		__overlay__ {
			status = "okay";
			bus-width = <2>;
		};
	};

	fragment@3 {
		target = <&sensor_csi_ch2>;
		__overlay__ {
			status = "okay";
			ports {
				status = "okay";
			};
		};
	};

	fragment@4 {
		target = <&sensor_csi_ch2_port0>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@5 {
		target = <&sensor_csi_ch2_in>;
		__overlay__ {
			status = "okay";
			bus-width = <2>;
			remote-endpoint = <&sensor_out_c>;
		};
	};

	fragment@6 {
		target = <&sensor_csi_ch2_port1>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@7 {
		target = <&sensor_csi_ch2_out>;
		__overlay__ {
			status = "okay";
		};
	};

