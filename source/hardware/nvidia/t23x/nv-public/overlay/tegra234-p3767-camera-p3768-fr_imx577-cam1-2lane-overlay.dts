					mode_type = "bayer";
					pixel_phase = "rggb";

					readout_orientation = "0";
					line_length = "4512";
					inherent_gain = "1";
					pix_clk_hz = "420000000";
					serdes_pix_clk_hz = "12000000000"; /* GMSL predefine */

					gain_factor = "1000";
					min_gain_val = "1000"; /* 0dB */
					max_gain_val = "356176"; /* 51dB */
					step_gain_val = "1";
					default_gain = "1000";
					min_hdr_ratio = "1";
					max_hdr_ratio = "64";
					framerate_factor = "1000000";
					min_framerate = "1500000"; /* 1.5 */
					max_framerate = "30000000"; /* 30 */
