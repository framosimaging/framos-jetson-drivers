					mode_type = "bayer";
					pixel_phase = "rggb";

					readout_orientation = "0";
					line_length = "4512";
					inherent_gain = "1";
					pix_clk_hz = "475200000";
					serdes_pix_clk_hz = "12000000000";/* GMSL predefine */

					gain_factor = "10";
					min_gain_val = "0";/* 0dB */
					max_gain_val = "480";/* 48dB */
					step_gain_val = "1";/* 0.1 */
					default_gain = "0";
					min_hdr_ratio = "1";
